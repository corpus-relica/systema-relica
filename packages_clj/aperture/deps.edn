                               org.clojure/test.check {:mvn/version "1.1.1"}
                               midje/midje {:mvn/version "1.10.9"}
                               org.clojure/tools.namespace {:mvn/version "1.4.4"}}
                  :main-opts ["-m" "io.relica.common.test.runner"]}
           :midje {:extra-paths ["test"]
