{
  "name": "@relica/3d-graph-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/3d-graph-ui.cjs",
  "module": "./dist/3d-graph-ui.js",
  "types": "./dist/App.d.ts",
  "exports": {
    ".": {
      "import": "./dist/3d-graph-ui.js",
      "require": "./dist/3d-graph-ui.cjs",
      "types": "./dist/App.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf ./dist && rm -f ./tsconfig.tsbuildinfo",
    "dev": "vite",
    "build:types": "tsc --project tsconfig.json",
    "build:vite": "vite build",
    "build": "yarn clean && yarn build:vite && yarn build:types",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "mobx-react": "^9.2.0",
    "ngraph.forcelayout": "^3.3.1",
    "ngraph.graph": "^20.0.1",
    "three": "^0.171.0",
    "@react-three/fiber": "^8.17.10",
    "@react-three/drei": "^9.120.4"
  },
  "devDependencies": {
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/three": "^0.171.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "concurrently": "^8.2.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "mobx": "^6.10.2",
    "nodemon": "^3.0.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "@relica/shared-testing": "workspace:*"
  }
}