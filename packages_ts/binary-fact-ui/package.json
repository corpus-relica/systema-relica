{
  "name": "@relica/binary-fact-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"yarn build:watch\"",
    "build": "tsc && vite build",
    "build:watch": "nodemon -w src -e ts,tsx --exec 'yarn build'",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@relica/types": "0.0.0",
    "@relica/fact-search-ui": "0.0.1",
    "@tanstack/react-query": "^5.7.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "axios": "^1.4.0",
    "concurrently": "^8.2.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "grommet": "^2.33.1",
    "grommet-icons": "^4.11.0",
    "nodemon": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.11",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-checker": "^0.6.2"
  },
  "main": "dist/binary-fact-ui.cjs",
  "module": "dist/binary-fact-ui.js"
}
